// Curve 0 is linear, from 0 to 1. v000=0 v127=1
// Curve 1 is bipolar, from -1 to 1.  v000=-1 v127=1
// Curve 2 is inverted linear, from 1 to 0. v000=1 v127=0
// Curve 3 bipolar inverted, from 1 to -1. v000=1 v127=-1
// Curve 4 concave (square the Curve 0 value).
// Curve 5 is Xfin power curve (square root of Curve 0 value).
// Curve 6 is Xfout power curve (square root of Curve 2 value).

<curve> curve_index=7 // Parabolic, scales by .9295 down to v001. Good for max attack 30 seconds.
// The right curve makes controls feel fluid but precise. Tiny attack left at v0 to soften clicks.
// This will NOT be audible if using this as a Decay curve, it's 10 samples or something similar.
v000=.0000005 v001=.0001 v003=.000116 v005=.000134 v007=.000155 v009=.000179 v11=.000207
v013=.00024 v015=.000278 v017=.000322 v019=.000372 v021=.000431 v023=.000499
v025=.000577 v027=.000668 v029=.000773 v031=.000895 v033=.00104 v035=.0012
v037=.00139 v039=.00161 v041=.00186 v043=.00215 v045=.00249 v047=.00288 v049=.00334
v051=.00386 v053=.00447 v055=.00518 v057=.00599 v059=.00693 v061=.00803 v063=.00929
v065=.0108 v067=.0124 v069=.0144 v071=.0167 v073=.0193 v075=.0223 v077=.0259
v079=.0299 v081=.0346 v083=.0401 v85=.0464 v87=.0537 v89=.0622 v91=.0719 v93=.0833
v95=.0964 v97=.112 v99=.129 v101=.149 v103=.173 v105=.2 v107=.232 v109=.268
v111=.31 v113=.359 v115=.416 v117=.481 v119=.557 v121=.645 v123=.747 v125=.864 v127=1

<control>
#define $MAX_AMP 60 // Maximum dB gain for Osc1/2/3/4/5 Volume. (This is self-balancing,
                    // you should not blow up your speakers if you mess with this.)
#define $MAX_ATK 30 // Maximum attack length, in seconds.
#define $MAX_DEC 30 // Maximum decay length, in seconds.
#define $MAX_SUS 30 // Maximum sustain level.
#define $MAX_REL 10 // Maximum release length, in seconds.

#define $XFIN_LO 12 // Minimum x-fade velocity for Hi-Sensitive oscillators.
#define $XFOUT_LO 3 // Minimum x-fade velocity for Lo-Sensitive oscillators.
#define $XFIN_HI 117 // Maximum x-fade velocity for Hi-Sensitive oscillators.
#define $XFOUT_HI 126 // Maximum x-fade velocity for Lo-Sensitive oscillators.
#define $XF_PAN 0 // For crossfading Pan settings: 127=full pan, 0=center it, in between blends.

#define $MODDEPTH 400 // How wide (in cents) is maximum modulation? Curve 4 so 50% = 1/4 of this depth.
#define $SINGLEBEND 200 // How wide (in cents) is per-group bending? 50%=neutral, swings both ways.
#define $PB_RANGE 1200 // How wide (in cents) is pitch bending? Swings both ways.

#define $LPFKEY_CENTER 84 // Key to center LPF scaling on (lower is brighter)
#define $LPFKEY_TRACK 100 // Cents of scaling per key (100 = equal)

// Column 1: Modulation and Mixing
set_cc1=0 label_cc1=∿1:Mod Depth ₀        // Normally at 0. Max Mod Depth defined above.❶
set_cc7=80 label_cc7=⇌7:Main Vol ₆₃       // Main Volume❶
set_cc13=63 label_cc13=∿13:Mod Rate ₅₀     // Mod rate, sqrt(x). 0 = 0.01 Hz, 127 = 21.99 Hz.❶
set_cc19=63 label_cc19=⇌19:❶Volume ₅₀
set_cc25=63 label_cc25=⇌25:❷Volume ₅₀
set_cc31=25 label_cc31=⇌31:❸Volume ₅₀
set_cc37=63 label_cc37=⇌37:❹Volume ₅₀
set_cc43=13 label_cc43=⇌43:❺Volume ₅₀

// Column 2: Oscillator 1 -- 8' Sine
set_cc2=9 label_cc2=2:❶Generator
set_cc8=63 label_cc8=8:❶Џ Bend ₅₀
set_cc14=2 label_cc14=14:❶⭧ Attack
set_cc20=127 label_cc20=20:❶⭨ Decay
set_cc26=127 label_cc26=26:❶⭬  Sustain
set_cc32=13 label_cc32=32:❶⭝ Release
set_cc38=63 label_cc38=38:❶Lowpass ₅₀
set_cc44=63 label_cc44=44:❶Pan/xf↹ ₅₀

// Column 3: Oscillator 2 -- Muted
set_cc3=127 label_cc3=3:❷Generator
set_cc9=63 label_cc9=9:❷Џ Bend ₅₀
set_cc15=2 label_cc15=15:❷⭧ Attack
set_cc21=127 label_cc21=21:❷⭨ Decay
set_cc27=127 label_cc27=27:❷⭬  Sustain
set_cc33=13 label_cc33=33:❷⭝ Release
set_cc39=63 label_cc39=39:❷Lowpass ₅₀
set_cc45=63 label_cc45=45:❷Pan/xf↹ ₅₀

// Column 4: Oscillator 3 -- Muted
set_cc4=127 label_cc4=4:❸Generator
set_cc10=63 label_cc10=10:❸Џ Bend ₅₀
set_cc16=2 label_cc16=16:❸⭧ Attack
set_cc22=127 label_cc22=22:❸⭨ Decay
set_cc28=127 label_cc28=28:❸⭬  Sustain
set_cc34=13 label_cc34=34:❸⭝ Release
set_cc40=63 label_cc40=40:❸Lowpass ₅₀ 
set_cc46=63 label_cc46=46:❸Pan/xf↹ ₅₀

// Column 5: Oscillator 4 -- Muted
set_cc5=127 label_cc5=5:❹Generator
set_cc11=63 label_cc11=11:❹Џ Bend ₅₀
set_cc17=2 label_cc17=17:❹⭧ Attack
set_cc23=127 label_cc23=23:❹⭨ Decay
set_cc29=127 label_cc29=29:❹⭬  Sustain
set_cc35=13 label_cc35=35:❹⭝ Release
set_cc41=63 label_cc41=41:❹Lowpass ₅₀ 
set_cc47=63 label_cc47=47:❹Pan/xf↹ ₅₀

// Column 6: Oscillator 5 -- Key noise
set_cc6=122 label_cc6=6:❺Generator ₉₆
set_cc12=63 label_cc12=12:❺Џ Bend ₅₀
set_cc18=2 label_cc18=18:❺⭧ Attack
set_cc24=20 label_cc24=24:❺⭨ Decay
set_cc30=0 label_cc30=30:❺⭬  Sustain
set_cc36=0 label_cc36=36:❺⭝ Release
set_cc42=63 label_cc42=42:❺Lowpass ₅₀
set_cc48=63 label_cc48=48:❺Pan/xf↹ ₅₀

<global>
xf_keycurve=power amp_veltrack=100 pitch_keycenter=60 ampeg_decay_shape=0 ampeg_decay_zero=0
ampeg_sustain=0 // This is necessary to make the Sustain controls work.
lfo01_pitch_curvecc1=4 // Curve of modulation depth is x² so 50% = 1/4 scale.
lfo01_freq_curvecc13=4 lfo01_freq=0.01 lfo01_freq_oncc13=21.99 // Modulation rate. 0 = 0.01 Hz, 127 = 21.99 Hz, 50% = 5.5 Hz because of sqrt(x) curve 4
lfo01_pitch_oncc1=$MODDEPTH // Modulation depth. Range set by #define above.
// CC7 is doing its default job, but specifying avoids glitchiness.
// The various "presets" will put their names above CC7's slot but it still controls volume.
amplitude_oncc7=100 amplitude_curvecc7=4
// Override the use of CC10 to perform global panning, so it can be re-used.
pan_oncc10=0 pan=0
// Octave bend range for primary Pitch Bend, both directions.
bend_up=$PB_RANGE bend_down=-$PB_RANGE
// Use center-neutral linear for pitch bends
pitch_curvecc8=1 pitch_curvecc9=1 pitch_curvecc10=1 pitch_curvecc11=1 pitch_curvecc12=1
// Use xfin curve for Osc Volume controls
volume_curvecc19=5 volume_curvecc25=5 volume_curvecc31=5
volume_curvecc37=5 volume_curvecc43=5
// Use fine curve for Attack controls
ampeg_attack_curvecc14=7 ampeg_attack_curvecc15=7 ampeg_attack_curvecc16=7
ampeg_attack_curvecc17=7 ampeg_attack_curvecc18=7
// Use fine curve for Decay controls
ampeg_decay_curvecc20=7 ampeg_decay_curvecc21=7 ampeg_decay_curvecc22=7
ampeg_decay_curvecc23=7 ampeg_decay_curvecc24=7
// Use square root curve for Sustain controls
ampeg_sustain_curvecc26=4 ampeg_sustain_curvecc27=4 ampeg_sustain_curvecc28=4
ampeg_sustain_curvecc29=4 ampeg_sustain_curvecc30=4
// Use square root curve for Release controls
ampeg_release_curvecc32=4 ampeg_release_curvecc33=4 ampeg_release_curvecc34=4
ampeg_release_curvecc35=4 ampeg_release_curvecc36=4
// Use center-neutral linear for pan controls
pan_curvecc44=1 pan_curvecc45=1 pan_curvecc46=1 pan_curvecc47=1 pan_curvecc48=1
// The entire keyboard is for notes.
lokey=0 hikey=127

// Oscillator 1 ░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░
<master> hicc44=0 xfout_lovel=$XFOUT_LO xfout_hivel=$XFOUT_HI pan_cc44=$XF_PAN // Lo-sensitive crossfade
// Oscillator 1 core
volume=-$MAX_AMP volume_cc19=$MAX_AMP
ampeg_attackcc14=$MAX_ATK ampeg_decaycc20=$MAX_DEC ampeg_sustaincc26=$MAX_SUS ampeg_releasecc32=$MAX_REL
fil_type=lpf_1p cutoff=20 cutoff_cc38=23980 fil_keytrack=$LPFKEY_TRACK fil_keycenter=$LPFKEY_CENTER

<region> locc2=0 hicc2=5 sample=*sine tune=-1200 pitch_cc8=$SINGLEBEND // 16' Sine
